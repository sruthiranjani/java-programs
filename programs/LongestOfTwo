public class LongestOfTwo {

    static int findLongest(int a, int b) {
        if (a > b)
            return a;
        else
            return b;
    }

    public static void main(String[] args) {

        int num1 = 25;
        int num2 = 40;

        System.out.println("Longest number = " + findLongest(num1, num2));
    }
}
